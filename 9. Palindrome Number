class Solution {
public:
    bool isPalindrome(int x) {
        
  long long   int n=x, l = 0, k;

  //cout << "Enter an integer: ";
 // cin >> n;

  while(n != 0) {
    k = n % 10;
    l = l * 10+k;
    n /= 10;
  }

  //cout << "Reversed Number = " << l;
  if(x>=0){
if(l==x){
  return true;}

  else return false;} else return false;
}
    
};
