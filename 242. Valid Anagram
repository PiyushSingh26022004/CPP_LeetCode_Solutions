class Solution
{
     public:
     bool isAnagram(string s, string t)
      {
        sort(s.begin(),s.end());
        sort(t.begin(),t.end());
        
           for(int i=0;i<s.size();i++)
            {
             if (s.size() > t.size()) return false;
               else if (s.size() < t.size()) return false;
          
                else  if(s[i]==t[i]) {}

                 else return false;
             }
        
             return true;  
       }
};
